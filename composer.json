{
    "name": "konsulting/laravel-extend-collections",
    "description": "A few extensions to the Laravel Collection and Arr.",
    "license": "MIT",
    "authors": [
        {
            "name": "Keoghan Litchfield",
            "email": "keoghan@klever.co.uk"
        }
    ],
    "require": {
        "php": "^7.0",
        "tightenco/collect": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Konsulting\\Laravel\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "classmap": ["tests"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Konsulting\\Laravel\\CollectionsServiceProvider"
            ]
        }
    }
}
